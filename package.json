{
  "name": "swiftui-for-web",
  "version": "1.1.0",
  "type": "module",
  "description": "A declarative UI framework that brings Apple's SwiftUI paradigm to web development. Zero dependencies, ~52KB gzipped.",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./core": {
      "types": "./src/index.d.ts",
      "import": "./src/core.js",
      "default": "./src/core.js"
    },
    "./charts": {
      "types": "./src/Charts/index.d.ts",
      "import": "./src/Charts/index.js",
      "default": "./src/Charts/index.js"
    },
    "./src/*": "./src/*"
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node run-tests.js",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "serve": "python3 -m http.server 8000 || python -m http.server 8000",
    "example:airbnb": "echo 'Open http://localhost:8000/Examples/Airbnb/' && npm run serve",
    "example:counter": "echo 'Open http://localhost:8000/Examples/Counter/' && npm run serve",
    "example:todo": "echo 'Open http://localhost:8000/Examples/TodoApp/' && npm run serve",
    "example:charts": "echo 'Open http://localhost:8000/Examples/Charts/' && npm run serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShawnBaek/SwiftUI-For-Web.git"
  },
  "homepage": "https://github.com/ShawnBaek/SwiftUI-For-Web#readme",
  "bugs": {
    "url": "https://github.com/ShawnBaek/SwiftUI-For-Web/issues"
  },
  "keywords": [
    "swiftui",
    "ui-framework",
    "declarative",
    "web",
    "javascript",
    "frontend",
    "mvvm",
    "reactive",
    "components",
    "no-dependencies",
    "zero-dependencies",
    "vanilla-js",
    "charts",
    "swift-charts",
    "data-visualization"
  ],
  "author": "Shawn Baek",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "sideEffects": false,
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "playwright": "^1.58.1"
  }
}
